master1 ansible_ssh_host=master1.openkube.io  
master2 ansible_ssh_host=master2.openkube.io  
master3 ansible_ssh_host=master3.openkube.io  

[infra]
infra1 ansible_ssh_host=infra1.openkube.io
infra2 ansible_ssh_host=infra2.openkube.io
infra3 ansible_ssh_host=infra3.openkube.io

[worker]
worker1 ansible_ssh_host=worker1.openkube.io

[all:vars]
